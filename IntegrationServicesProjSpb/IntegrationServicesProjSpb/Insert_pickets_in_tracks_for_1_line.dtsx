                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @LayoutMin as int
SET @LayoutMin = 100000;

DECLARE @LayoutMax as int
SET @LayoutMax = 200000;

DECLARE @TrackDisplayed as int
SET @TrackDisplayed = 1

DECLARE @LineNumberDisplayed as int
SET @LineNumberDisplayed = 1

DECLARE @TrackId as int

SELECT @TrackId = ?

DECLARE @dlina_standart as int
SET @dlina_standart = 100000 

declare @max_index as int
set @max_index = 0;

--declare @forrownumber table(point  int, track  int);

--insert into @forrownumber SELECT distinct Point, [Track] FROM [SpbMetrocardOrigin].[dbo].[Main] where Track &lt; @TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax ;

--SELECT @max_index= count([Point])  FROM @forrownumber;

--SELECT @max_index= count(distinct [Point]) FROM [SpbMetrocardOrigin].[dbo].[Main] where Track = @TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax;

DECLARE @PicketsTempForCreateID table(Number nvarchar(50) );
DECLARE @PicketsTemp table(Npiketa nvarchar(50) , Dlina int, NpicketBefore int, NpicketAfter int, Number int, [path] int, StartShiftLine int, EndShiftLine int, NumberTemp int);
declare @max_number_picket as int;
select @max_number_picket = MAX([Point]) from [SpbMetrocardOrigin].[dbo].[Main] where Track =@TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax ;

declare @min_number_picket as int;
select @min_number_picket = MIN([Point]) from [SpbMetrocardOrigin].[dbo].[Main] where Track =@TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax ;



-----------------------------------------------------------------------------------------
		-- Reconstruct All pickets numbers --
-----------------------------------------------------------------------------------------

declare @needed_pickets_count as int;
set @needed_pickets_count=0;

if @min_number_picket &lt; 0 and @max_number_picket &gt; 0
begin
	set @needed_pickets_count =  @max_number_picket;
	set @min_number_picket = @min_number_picket;
end
else
begin
	if @min_number_picket &gt; 0 and @max_number_picket &gt; 0
	begin
		set @needed_pickets_count = @max_number_picket;
		set @min_number_picket = 0;
	end
	else
	begin
		if @min_number_picket &lt; 0 and @max_number_picket &lt; 0
		begin
			set @needed_pickets_count = @min_number_picket;
			set @min_number_picket = @min_number_picket;			
		end
	end	
end


;with cteNums(id,n,dlina, number_picket_str, track) AS
(
    SELECT @max_index,  @min_number_picket, @dlina_standart , cast(@min_number_picket as nvarchar(50)), @TrackId
    UNION ALL
    SELECT id + 1, n+1, dlina, cast((n + 1) as nvarchar(50)), @TrackId
    FROM cteNums WHERE n &lt;  @needed_pickets_count -- how many times to iterate
)
INSERT INTO @PicketsTemp(NumberTemp,Dlina,Npiketa,[path], Number, StartShiftLine, EndShiftLine)
SELECT n,dlina, number_picket_str, track, id, 0, 0 FROM cteNums option ( MaxRecursion 0 );

--select * from @PicketsTemp;

--insert into @PicketsTemp(NumberTemp) SELECT distinct [Point] FROM [SpbMetrocardOrigin].[dbo].[Main] where Track =@TrackId and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax order by Point;

----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
-- Sets next columns: Dlina, path, Npiketa(picket display number), Number(Primary key) --
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------






;with t as(
	select NpicketAfter, NpicketBefore, LAG(Number,1,0) over(order by NumberTemp) as prev_number, LEAD(Number,1,0) over(order by NumberTemp) as next_number 
	from @PicketsTemp 
)
update t
set t.NpicketBefore = t.prev_number,
	t.NpicketAfter = t.next_number


----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
					-- Sets length for not standart pickets --
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

;with t as(
	select Dlina, NumberTemp from @PicketsTemp
)
update t
set t.Dlina = src.Dlina *1000
FROM (SELECT [Npiketa] ,[Dlina] ,[Перегон] FROM [SpbMetrocardOrigin].[dbo].[1_1] where Dlina != 100 and [Перегон] &gt; @LayoutMin and [Перегон] &lt; @LayoutMax) src
where t.NumberTemp = src.Npiketa


;with t as(
	select Dlina, NumberTemp from @PicketsTemp
)
update t
set t.Dlina = src.[LENGTH] *1000
FROM (SELECT [N] ,[LENGTH] FROM [SpbMetrocardOrigin].[dbo].[1-2_PICK] where [LENGTH] != 100 ) src
where t.NumberTemp = src.N

----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
					-- Sets offsets by line start --
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

;with t as(
	select Dlina,StartShiftLine,EndShiftLine, NumberTemp, LAG(EndShiftLine,1,0) over(order by  NumberTemp ) as previousEnd from @PicketsTemp where NumberTemp = 0 
)
update t
	set t.StartShiftLine = 0,
		t.EndShiftLine = Dlina

	
;with t as(
select Dlina,StartShiftLine,EndShiftLine, NumberTemp , SUM( StartShiftLine + Dlina)  over(order by NumberTemp asc) as curEnd  from @PicketsTemp where NumberTemp &gt;= 0
)
update t
	set t.EndShiftLine = t.curEnd




;with t as(
	select Dlina,StartShiftLine,EndShiftLine, NumberTemp, LAG(EndShiftLine,1,0) over(order by  NumberTemp asc ) as previousEnd from @PicketsTemp where NumberTemp &gt;= 0
)
update t
	set t.StartShiftLine = t.previousEnd




----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
					--  Sets pickets left and right boundary  --
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------



;with t as (
	select * from @PicketsTemp
)
update t set t.NpicketBefore = -2147483648 where Number = @max_index;

;with t as (
	select * from @PicketsTemp
)
update t set t.NpicketAfter = 2147483647 where NumberTemp = @needed_pickets_count;
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
					-- Sets -0 picket --
----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

declare @before_zero_picket as int;
declare @after_zero_picket as int;
declare @length_zero_picket as int;
declare @zero_picket_number as int;
declare @zero_picket_path as int;

select @length_zero_picket= Dlina ,@before_zero_picket = NpicketBefore, @after_zero_picket = NpicketAfter ,@zero_picket_number = Number, @zero_picket_path = [path]
from @PicketsTemp 
where NumberTemp = 0;



select @max_index = MAX(NumberTemp) from @PicketsTemp;


if ISNULL(@zero_picket_number,0)!=0
begin
	declare @max_picket_number as int;
	select @max_picket_number = MAX(Number) from @PicketsTemp;

	declare @minus_zero_picket as int;
	set @minus_zero_picket = @max_picket_number + 1;

	insert into @PicketsTemp values(N'-0', @length_zero_picket, @before_zero_picket, @zero_picket_number, @minus_zero_picket, @zero_picket_path, 0, -@length_zero_picket, @minus_zero_picket);
	update @PicketsTemp set NpicketBefore = @minus_zero_picket where Number = @zero_picket_number;	
end

;with t as(
	select Dlina, StartShiftLine, EndShiftLine, NumberTemp , SUM( StartShiftLine - Dlina)  over(order by NumberTemp desc) as curEnd  from @PicketsTemp where NumberTemp &lt; 0
)
update t
	set t.EndShiftLine = t.curEnd - @length_zero_picket


;with t as(
	select Dlina,StartShiftLine,EndShiftLine, NumberTemp, LAG(EndShiftLine,1,0) over(order by  NumberTemp desc ) as previousEnd from @PicketsTemp where NumberTemp &lt; 0
)
update t
	set t.StartShiftLine = t.previousEnd 

update @PicketsTemp set StartShiftLine = -@length_zero_picket where NumberTemp = -1;

select * from @PicketsTemp;</property>
