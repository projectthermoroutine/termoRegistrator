                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @LayoutMin as int
SET @LayoutMin = ?;

DECLARE @LayoutMax as int
SET @LayoutMax = ?;

DECLARE @TrackDisplayed as int
SET @TrackDisplayed = 4

DECLARE @LineNumberDisplayed as int
SET @LineNumberDisplayed = 3

DECLARE @TrackId as int

SELECT @TrackId = ?;
DECLARE @dlina_standart as int
SET @dlina_standart = 100000 


declare @max_index as int
set @max_index = cast( ? as int)


DECLARE @PicketsTempForCreateID table(Number nvarchar(50) );
DECLARE @PicketsTemp table(Npiketa nvarchar(50) , Dlina int, NpicketBefore int, NpicketAfter int, Number int, [path] int, StartShiftLine int, EndShiftLine int, NumberTemp int, Count int);
DECLARE @PicketsTempAfter table(Npiketa nvarchar(50) , Dlina int, NpicketBefore int, NpicketAfter int, Number int, [path] int, StartShiftLine int, EndShiftLine int);
DECLARE @PicketsTempBefore table(Npiketa nvarchar(50) , Dlina int, NpicketBefore int, NpicketAfter int, Number int, [path] int, StartShiftLine int, EndShiftLine int);

-- Process begin --

insert into @PicketsTempAfter(Number)   SELECT distinct [Point] FROM [SpbMetrocardOrigin].[dbo].[Main] where Track = @TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax  order by Point  offset 1 rows;
insert into @PicketsTempAfter(Number)   VALUES(2147483647);

insert into @PicketsTempBefore(Number)   VALUES(-2147483648);
insert into @PicketsTempBefore(Number)   SELECT distinct [Point] FROM [SpbMetrocardOrigin].[dbo].[Main] where Track = @TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax  order by Point;

insert into @PicketsTemp(NumberTemp) SELECT distinct [Point] FROM [SpbMetrocardOrigin].[dbo].[Main] where Track = @TrackDisplayed and Layout &gt; @LayoutMin and Layout &lt; @LayoutMax order by Point;

-----------------------------------------------------------------------------------------
-- Sets next columns: Dlina, path, Npiketa(picket display number), Number(Primary key) --
-----------------------------------------------------------------------------------------

;with t as(
	select Number, Npiketa , [path], Dlina, NpicketAfter, NpicketBefore, StartShiftLine, EndShiftLine, row_number() over(order by NumberTemp) as indx 
	from @PicketsTemp
)
update t
set t.Npiketa = cast(n.NumberTemp as nvarchar(50)),
	t.[path] = @TrackId,
	t.Dlina = @dlina_standart,
	t.Number = n.indx+ @max_index,
	t.StartShiftLine = 0,
	t.EndShiftLine = 0
from (select NumberTemp , row_number() over(order by NumberTemp) as indx from @PicketsTemp ) n ,
	 (select Number, row_number() over(order by Number) as rowNumber from @PicketsTempAfter ) pa,
	 (select Number, row_number() over(order by Number) as rowNumber from @PicketsTempBefore ) pb
where t.indx = n.indx  and t.indx = pa.rowNumber and t.indx = pb.rowNumber


;with t as(
	select NpicketAfter, NpicketBefore, LAG(Number,1,0) over(order by NumberTemp) as prev_number, LEAD(Number,1,0) over(order by NumberTemp) as next_number 
	from @PicketsTemp 
)
update t
set t.NpicketBefore = t.prev_number,
	t.NpicketAfter = t.next_number


-----------------------------------------------------------------------------------------
					-- Sets length for not standart pickets --
-----------------------------------------------------------------------------------------


-----------------------------------------------------------------------------------------
					-- Sets offsets by line start --
-----------------------------------------------------------------------------------------

;with t as(
	select Dlina,StartShiftLine,EndShiftLine, NumberTemp , SUM( StartShiftLine + Dlina)  over(order by NumberTemp) as curEnd  from @PicketsTemp
)
update t
	set t.EndShiftLine = t.curEnd

;with t as(
	select Dlina,StartShiftLine,EndShiftLine, NumberTemp, LAG(EndShiftLine,1,0) over(order by NumberTemp) as previousEnd from @PicketsTemp
)
update t
	set t.StartShiftLine = t.previousEnd
	
select * from @PicketsTemp;</property>
